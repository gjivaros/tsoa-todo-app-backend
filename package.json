    "express": "^4.18.2",
    "tsoa": "^6.0.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2"
  }
